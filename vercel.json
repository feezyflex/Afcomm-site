
{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { 
          "framework": "vite"
        }
      }
    ],
      "routes": [
    {
      "src": "/*",
      "dest": "index.html"
    }, 
    {
      "src": "/installations",
      "dest": "index.html"
    },
    {
      "src": "/support&maintenances",
      "dest": "index.html"
    },
    {
      "src": "/supply&procurements",
      "dest": "index.html"
    },
    {
      "src": "/products",
      "dest": "index.html"
    },
    {
      "src": "/contact",
      "dest": "index.html"
    }
      ]
  }